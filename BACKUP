<script>
  const statusMenuItems = document.querySelectorAll(".status-menu-item");
const supervisorImage = document.querySelector("#supervisor-info .status-img");
const supervisorName = document.querySelector("#supervisor-info .status-menu-item");

statusMenuItems.forEach(item => {
  item.addEventListener("click", function() {
    const equipe = item.getAttribute("data-status");

    // Aqui você pode mapear as equipes para os respectivos supervisores e imagens
    const supervisores = {
      "EQUIPE A": {
        nome: "SUPERVISOR 1º SGT FULANO",
        imagem: "https://cdn.icon-icons.com/icons2/876/PNG/512/female-user-profile-on-round-button_icon-icons.com_68317.png"
      },
      "EQUIPE B": {
        nome: "SUPERVISOR 1º SGT BELTRANO",
        imagem: "https://cdn.icon-icons.com/icons2/876/PNG/512/woman-with-dark-shirt_icon-icons.com_68266.png"
      },
      "EQUIPE C": {
        nome: "SUPERVISOR 1º SGT CACIQUE",
        imagem: "https://cdn.icon-icons.com/icons2/876/PNG/512/user-round-button_icon-icons.com_68279.png"
      },
      "EQUIPE D": {
        nome: "SUPERVISOR 1º SGT INDIO",
        imagem: "https://cdn.icon-icons.com/icons2/876/PNG/512/female-user-with-dress-and-flower-on-hair_icon-icons.com_68315.png"
      },
      "EQUIPE E": {
        nome: "SUPERVISOR 1º SGT TRIBO",
        imagem: "https://cdn.icon-icons.com/icons2/876/PNG/512/modern-male-user-with-sunglasses_icon-icons.com_68293.png"
      },
      // Adicione as demais equipes aqui
    };

    // Verifica se a equipe selecionada está no objeto de supervisores
    if (supervisores[equipe]) {
      const supervisor = supervisores[equipe];
      supervisorName.textContent = supervisor.nome;
      supervisorImage.src = supervisor.imagem;
    }
  });
});

</script>
